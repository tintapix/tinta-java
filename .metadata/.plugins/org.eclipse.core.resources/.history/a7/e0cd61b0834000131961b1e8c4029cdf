package Molygon;

public interface molyIWindow {

	
	public String  getWndName();
	
	/*
	 * Parsing window position from config file, using  getWndName()	
	 * */
	public molyPosition getWndPos();
	
	/*
	 * Parsing window size from config file, using  getWndName()	
	 * */
	public molyPosition getWndSize();
	
	
	
	
}
