package Molygon;


public class molyImgBuffer {
	private int mWidth;
	private int mHeight;
	private int mSize;
	
	private byte[] mBuffer;
	
	public molyImgBuffer(){
		mWidth = -1;
		mHeight = -1;
		mSize = -1;
		mBuffer = new byte[]{};		
	}
	
	
	public molyImgBuffer(int mWidth, ){
		mWidth = -1;
		mHeight = -1;
		mSize = -1;
		mBuffer = new byte[]{};		
	}
	
	
	
}
